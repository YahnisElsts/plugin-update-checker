[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[*.php]
indent_style = tab
eclint_block_comment_start = /*
eclint_block_comment = *
eclint_block_comment_end = */

[*.js]
indent_style = tab

[README.md]
# doctoc + TAB-s
indent_style = unset
